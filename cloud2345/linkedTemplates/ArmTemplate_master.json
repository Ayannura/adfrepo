{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "cloud2345"
		},
		"AzureSqlDatabase1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase1'"
		},
		"abhisheksqlcloud_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'abhisheksqlcloud'"
		},
		"cloud22_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'cloud22'"
		},
		"datalakeayangen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'datalakeayangen2'"
		},
		"datalakeayangen2345_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'datalakeayangen2345'"
		},
		"datastorage1223_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'datastorage1223'"
		},
		"file678blob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'file678blob'"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://keyvault21.vault.azure.net/"
		},
		"datalakeayangen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalakeayangen2.dfs.core.windows.net/"
		},
		"datalakeayangen2345_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalakeayangen2345.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "cloud2345_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlDatabase1_connectionString": {
						"value": "[parameters('AzureSqlDatabase1_connectionString')]"
					},
					"abhisheksqlcloud_connectionString": {
						"value": "[parameters('abhisheksqlcloud_connectionString')]"
					},
					"cloud22_connectionString": {
						"value": "[parameters('cloud22_connectionString')]"
					},
					"datalakeayangen2_accountKey": {
						"value": "[parameters('datalakeayangen2_accountKey')]"
					},
					"datalakeayangen2345_accountKey": {
						"value": "[parameters('datalakeayangen2345_accountKey')]"
					},
					"datastorage1223_connectionString": {
						"value": "[parameters('datastorage1223_connectionString')]"
					},
					"file678blob_connectionString": {
						"value": "[parameters('file678blob_connectionString')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"datalakeayangen2_properties_typeProperties_url": {
						"value": "[parameters('datalakeayangen2_properties_typeProperties_url')]"
					},
					"datalakeayangen2345_properties_typeProperties_url": {
						"value": "[parameters('datalakeayangen2345_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "cloud2345_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'cloud2345_ArmTemplate_0')]"
			]
		}
	]
}